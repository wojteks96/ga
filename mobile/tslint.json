{
  "extends": [
    "tslint:recommended",
    "tslint-eslint-rules",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "interface-name": false,
    "jsx-no-lambda": false,
    "object-literal-sort-keys": false,
    "quotemark": [
      true,
      "double",
      "jsx-double"
    ],
    "semicolon": [
      true,
      "always",
      "strict-bound-class-methods"
    ]
  }
}
